{
  "name": "Picture",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string"
    },
    "url": {
      "type": "string",
      "required": true
    },
    "created": {
      "type": "date",
      "defaultFn": "now"
    },
    "modified": {
      "type": "date",
      "defaultFn": "now"
    }
  },
  "validations": [],
  "relations": {
    "imageable": {
      "type": "belongsTo",
      "polymorphic": true
    }
  },
  "acls": [],
  "methods": {}
}
