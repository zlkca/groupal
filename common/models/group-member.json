{
  "name": "GroupMember",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "groupId": {
      "type": "number"
    },
    "userId": {
      "type": "number"
    },
    "role": {
      "type": "string",
      "default": "member"
    },
    "status": {
      "type": "string",
      "default": "active"
    },
    "created": {
      "type": "date",
      "defaultFn": "now"
    },
    "modified": {
      "type": "date",
      "defaultFn": "now"
    }
  },
  "validations": [],
  "relations": {
    "group": {
      "type": "belongsTo",
      "model": "Group",
      "foreignKey": "groupId"
    },
    "user": {
      "type": "belongsTo",
      "model": "User",
      "foreignKey": "userId"
    }
  },
  "acls": [],
  "methods": {}
}
